<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:ScanProject.Shared.Converter">

    <converter:UriToImageConverter x:Key="LocalUriToImageConverter"/>
    <converter:BooleanToStringConverter FalseValue="Không sử dụng" TrueValue="Đang sử dụng" x:Key="converterIsUsedToString" />
    <converter:BooleanToVisibilityConverter FalseValue="Collapsed"  TrueValue="Visible" x:Key="booleanToVisibilityConverter" />
    <converter:InverseBooleanConverter x:Key="inverseBooleanConverter" />
    <converter:NullToVisibilityConverter x:Key="nullToVisibilityConverter" NotNull="Visible" Null="Collapsed" />
    <converter:ZeroToNullConverter x:Key="zeroToNullConverter" Zero="" />
    <converter:EnumToBooleanConverterGeneral x:Key="enumToBooleanConverter" />
    <converter:StringToDoubleConverter x:Key="stringToDoubleConverter" />

</ResourceDictionary>